{
	"name": "@salamander.be/vue-image-previewer",
	"description": "A VueJs previewer for images that are not included in the component.",
	"author": "Frederiek <frederiek@salamander.be>",
	"license": "MIT",
	"version": "0.0.1",
	"private": false,
	"main": "./dist/vue-image-previewer.common.js",
	"keywords": [
		"vue",
		"image-previewer",
		"gallery"
	],
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"build-bundle": "vue-cli-service build --target lib --name vue-image-previewer ./src/components/index.js"
	},
	"dependencies": {
		"vue": "^2.5.17"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.0.5",
		"@vue/cli-plugin-eslint": "^3.0.5",
		"@vue/cli-service": "^3.0.5",
		"vue-template-compiler": "^2.5.17",
		"node-sass": "^4.9.0",
		"sass-loader": "^7.0.1"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"files": [
		"dist/*",
		"src/*",
		"public/*",
		"*.json",
		"*.js"
	],
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	]
}
